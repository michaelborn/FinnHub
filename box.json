{
    "author":"Michael Born <michael@ortussolutions.com>",
    "dependencies":{
        "hyper":"^3.1.0"
    },
    "description":"Wraps the FinnHub API",
    "devDependencies":{
        "coldbox":"6.4.0",
        "testbox":"^4.4.0-snapshot"
    },
    "ignore":[
        "**/.*",
        "/test/",
        "/tests/"
    ],
    "installPaths":{
        "coldbox":"tests/resources/app/coldbox/",
        "testbox":"tests/testbox/",
        "hyper":"modules/hyper/"
    },
    "location":"ForgeboxStorage",
    "name":"FinnHub",
    "private":false,
    "repository":{
        "type":"git",
        "URL":"https://github.com/michaelborn_me/finnhub"
    },
    "bugs":"https://github.com/michaelborn_me/finnhub/issues",
    "scripts":{
        "postVersion":"package set location='michaelborn_me/finnhub#v`package version`'",
        "onRelease":"publish",
        "postPublish":"!git push --follow-tags"
    },
    "shortDescription":"CFML wrapper for the FinnHub API.",
    "slug":"finnhub",
    "testbox":{
        "runner":[
            {
                "default":"http://localhost:6335/test-harness/runner.cfm"
            }
        ]
    },
    "type":"modules",
    "version":"1.0.0"
}
